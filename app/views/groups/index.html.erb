<h1> new group </h1>
<%= render 'form', group: @group %>


 
<h1>Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @currentUser = current_user %>
    <% @groups.each do |group| %>
      <tr>
        <td><%= group.name %></td>
        <td><%= group.user.name %></td>
       
       
      </tr>
    <% end %>
  </tbody>
</table>

